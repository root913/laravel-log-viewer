{
    "name": "root913/laravel-log-viewer",
    "description": "A Laravel log reader",
    "license": "MIT",
    "keywords": ["log", "log-reader", "log-viewer", "logging", "laravel", "lumen"],
    "type": "laravel-package",
    "authors": [
        {
            "name": "root913",
            "email": "krzycho1717@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*|5.*|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "orchestra/testbench": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-4": {
            "root913\\LaravelLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "root913\\LaravelLogViewer\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLogViewerServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
